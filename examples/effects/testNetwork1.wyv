require stdout
import effects.network1

resource type NetworkType1
  effect send
  effect receive
  def sendData(data : String) : {} Unit
  def receiveData() : {receive} Unit

val n1 : NetworkType1 = network1(stdout)
n1.sendData("Network1 with effects")	// print "data sent: Network1 with effects"
n1.receiveData()		// print "data received"