require stdout

import wyvern.Int
import wyvern.collections.list

type List = list.List

//find the min of list, add it to new list (counting from 0)
def min(list: List, min:Int, count:Int):Int
    val n = list.length()
    if (n - 1 == count)
        min
	
    val item: Int = list.get(count).getOrElse(()=> -1)
    var minV: Int = min
    
    if (item < minV)
        minV = item
    min(list,min,count+1)


    
    
