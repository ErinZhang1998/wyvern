module Server

def sendHeaders(dos:Dyn):Unit
    dos.writeBytes("HTTP/1.0 200 OK\r\n")
    dos.writeBytes("Content-Type: text/html\r\n")
    dos.writeBytes("\r\n")

def welcomeMsg(dos:Dyn):Unit
    dos.writeBytes("<html><h1>Welcome to the Wyvern Server</h1></html>\r\n")

def flush(dos:Dyn):Unit
    dos.flush()
	
def sendHtmlFile(dos:Dyn, str: String):Unit
    dos.writeBytes(str)

    