<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="false" sync-formatter="false">
  <local-check-config name="wyvern_checkstyle" location="checkstyle.xml" type="external" description="">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="false" check-config-name="wyvern_checkstyle" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
  <fileset name="all-non-generated-files" enabled="true" check-config-name="wyvern_checkstyle" local="true">
    <file-match-pattern match-pattern=".java$" include-pattern="true"/>
    <file-match-pattern match-pattern="wyvern/tools/lexing/archlexer/ArchLexer.java" include-pattern="false"/>
    <file-match-pattern match-pattern="wyvern/tools/lexing/WyvernLexer.java" include-pattern="false"/>
    <file-match-pattern match-pattern="wyvern/tools/parsing/coreparser/ArchParserConstants.java" include-pattern="false"/>
    <file-match-pattern match-pattern="wyvern/tools/parsing/coreparser/Token.java" include-pattern="false"/>
    <file-match-pattern match-pattern="wyvern/tools/parsing/coreparser/TokenManager.java" include-pattern="false"/>
    <file-match-pattern match-pattern="wyvern/tools/parsing/coreparser/TokenMgrError.java" include-pattern="false"/>
    <file-match-pattern match-pattern="wyvern/tools/parsing/coreparser/WyvernParser.java" include-pattern="false"/>
    <file-match-pattern match-pattern="wyvern/tools/parsing/coreparser/WyvernParserConstants.java" include-pattern="false"/>
  </fileset>
</fileset-config>
