resource module Lists

type Any

type List
    def isEmpty() : Boolean
    def getVal() : Any
    def getNext() : List

def empty(emptyVal : Any) : List = new
    def isEmpty() : Boolean = true
    def getVal() : Any = emptyVal
    def getNext() : List = this

def cons(x : Any, y : List) : List = new
    def isEmpty() : Boolean = false
    def getVal() : Any = x
    def getNext() : List = y

